<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ambedo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Metal"
            android:label="@string/metal"
            android:parentActivityName="com.example.android.ambedo.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.ambedo.MainActivity" />
        </activity>
        <activity
            android:name=".Hiphop"
            android:label="@string/hiphop"
            android:parentActivityName="com.example.android.ambedo.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.ambedo.MainActivity" />
        </activity>
        <activity
            android:name=".Rock"
            android:label="@string/rock"
            android:parentActivityName="com.example.android.ambedo.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.ambedo.MainActivity" />
        </activity>
    </application>

</manifest>